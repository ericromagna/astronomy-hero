{
  "expo": {
    "name": "Astronomy Hero",
    "slug": "astronomy-hero-ui",
    "version": "1.0.2",
    "orientation": "portrait",
    "icon": "./assets/custom-icon.png",
    "splash": {
      "image": "./assets/custom-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/custom-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.ericromagna.astronomyheroui"
    },
    "web": {
      "favicon": "./assets/custom-icon.png"
    }
  }
}
